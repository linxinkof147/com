<view class="body"><input class="uni-input common-input" type="text" placeholder="输入需要绑定的邮箱" data-event-opts="{{[['input',[['__set_model',['','email','$event',[]]]]]]}}" value="{{email}}" onInput="__e"/><button class="{{(('user-set-btn')+' '+((disabled||isbind)?'user-set-btn-disable':''))}}" loading="{{loading}}" type="primary" disabled="{{disabled||isbind}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" onTap="__e">{{isbind?"已绑定":"立即绑定"}}</button></view>