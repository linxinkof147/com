<view><view style="display:flex;align-items:center;padding:0 20rpx;height:88rpx;position:fixed;z-index:9999;left:0;top:0;right:0;background:#FFFFFF;"><view class="iconfont icon-sousuo" style="position:absolute;left:30rpx;color:#CCCCCC;"></view><input style="flex:1;padding:5rpx 0 5rpx 50rpx;border-radius:4px;background:#F7F7F7;" type="text" placeholder="{{getPlaceholder}}" placeholder-style="color: #CCCCCC;" data-event-opts="{{[['confirm',[['getdata',['$event']]]],['input',[['__set_model',['','searchtext','$event',[]]]]]]}}" value="{{searchtext}}" onConfirm="__e" onInput="__e"/><text data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}" style="padding-left:20rpx;" onTap="__e">取消</text></view><view style="height:88rpx;"></view><block a:if="{{list.length>0}}"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><block><block a:if="{{searchType=='post'}}"><index-list vue-id="{{'1-'+index}}" item="{{item}}" index="{{index}}" data-event-opts="{{[['^changeevent',[['updateData']]]]}}" onChangeevent="__e" onVueInit="__l"></index-list></block><block a:else><block a:if="{{searchType=='topic'}}"><view style="padding:0 20rpx;"><topic-list vue-id="{{'2-'+index}}" item="{{item}}" index="{{index}}" onVueInit="__l"></topic-list></view></block><block a:else><view style="padding:0 20rpx;"><user-list vue-id="{{'3-'+index}}" item="{{item}}" index="{{index}}" hidden onVueInit="__l"></user-list></view></block></block></block></block><load-more vue-id="4" loadtext="{{loadtext}}" onVueInit="__l"></load-more></block><block a:else><block a:if="{{issearch&&list.length<1}}"><no-thing vue-id="5" onVueInit="__l"></no-thing></block></block></view>